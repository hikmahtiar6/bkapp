	public function size($size){
		if($size >= 1073741824){
			return round($size/1073741824, 1)." GB";
		}elseif($size >= 1048576){
			return round($size/1048576, 1)." MB";
		}elseif($size >= 1024){
			return round($size/1024, 2)." KB";
		}else{
			return $size." B";
		}
	}
